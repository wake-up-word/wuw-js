{
    "name": "fft-as",
    "packageManager": "yarn@3.0.2",
    "dependencies": {
        "@assemblyscript/loader": "^0.19.17"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.3.0",
        "@typescript-eslint/parser": "^5.3.0",
        "assemblyscript": "^0.19.17",
        "eslint": "^7.1.0",
        "typescript": "^4.4.4"
    },
    "scripts": {
        "build": "yarn asbuild:debug",
        "asbuild:untouched": "asc assembly/index.ts --sourceMap --debug --exportRuntime --target debug ",
        "asbuild:optimized": "asc assembly/index.ts --exportRuntime --target release",
        "asbuild": "yarn asbuild:untouched && yarn asbuild:optimized",
        "test": "node tests"
    }
}
